<%= form_for(resource, as: resource_name, url: session_path(resource_name), builder: UserFormBuilder) do |f| %>
  <div class="panel-body bg-light p30">
    <div class="row">
      <div class="col-sm-7 pr30">
        <div class="section row hidden">
          <%= render "devise/shared/omniauth_links" %>
        </div>
        <%= devise_error_messages! %>

        <%= f.email_field :email, autofocus: true, placeholder: "Enter email", class: "gui-input", icon: 'user' %>
        <%= f.password_field :password, autocomplete: 'off', class: "gui-input", icon: 'lock' %>
      </div>
      <%= render "devise/shared/sign_up_benefits" %>
    </div>
  </div>
  <div class="panel-footer clearfix p10 ph15">
    <%= f.submit 'Sign in' %>
    <% if devise_mapping.rememberable? -%>
      <%= f.switch_box :remember_me, checked: true %>
    <% end %>
  </div>
<% end %>
